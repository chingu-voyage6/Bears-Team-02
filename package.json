{
  "name": "Founder-Connect",
  "version": "1.0.0",
  "description": "A place to connect Technical and Non-technical Founders",
  "main": "index.js",
  "engines": {
    "node": "15.2.1",
    "npm": "6.14.9"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "test": "nodemon --exec 'mocha --recursive'",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chingu-voyage6/Bears-Team-02.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chingu-voyage6/Bears-Team-02/issues"
  },
  "homepage": "https://github.com/chingu-voyage6/Bears-Team-02#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "jwt-simple": "^0.5.6",
    "mongoose": "^5.11.11",
    "object-diff": "0.0.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "mocha": "^8.2.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7"
  }
}
